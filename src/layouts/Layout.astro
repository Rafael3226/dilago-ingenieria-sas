---
import Footer from "../components/Footer.astro";
import Header from "../components/Header.astro";

import "../assets/css/style.css";
import "../assets/css/font-icons.css";
import "../assets/css/onepage.css";
import "../assets/css/home.css";
import "../assets/css/util.css";
import "../assets/css/main.css";
import "../../node_modules/owl.carousel/dist/assets/owl.carousel.min.css";

import IconImg from "../assets/img/icon.png";
---

<html dir="ltr" lang="en-US">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="x-ua-compatible" content="IE=edge" />
    <meta name="author" content="" />
    <meta name="description" content="Dilago Ingeniería pagina web" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link rel="icon" href={IconImg.src} />
    <title>Dilago Ingeniería</title>
  </head>
  <body
    class="stretched side-push-panel device-xxl device-up-xxl device-up-xl device-up-lg device-up-md device-up-sm device-up-xs is-expanded-menu has-plugin-hoveranimation has-plugin-html5video has-plugin-onepage has-plugin-bootstrap has-plugin-isotope has-plugin-flexslider has-plugin-carousel has-plugin-textrotator has-plugin-form template-contactform-ready has-plugin-scrolldetect has-plugin-gmap has-plugin-cookie has-plugin-bscomponents has-plugin-notify"
  >
    <div id="wrapper">
      <Header />
      <slot />
      <Footer />
    </div>
    <script is:inline src="/assets/js/functions.bundle.js"></script>
    <script is:inline src="/assets/js/plugins.min.js"></script>
    <script is:inline src="/node_modules/jquery/dist/jquery.js"></script>
    <script is:inline src="/node_modules/owl.carousel/dist/owl.carousel.min.js"
    ></script>
    <script is:inline src="/public/assets/js/clients-carousel.js"></script>
  </body>
</html>
